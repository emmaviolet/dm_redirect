{
  "name": "dm_redirect",
  "version": "1.0.0",
  "description": "Source code for Chrome browser extension [DM Redirect](https://chrome.google.com/webstore/detail/pfbdhjecpnbopodopafogoggcbpojdci)",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "sinon": "^7.1.0",
    "sinon-chrome": "^3.0.1",
    "testcafe": "^1.2.0"
  },
  "scripts": {
    "features": "testcafe chrome \"test/features/**.js\"",
    "headless_features": "testcafe chrome:headless \"test/features/**.js\"",
    "lint": "eslint app/js/**.js test/*/**.js",
    "start": "browserify-directory app/js lib",
    "test": "npm run specs && npm run features",
    "specs": "mocha --require ./test/setup.js --recursive -R dot \"test/specs/*_spec.js\""
  },
